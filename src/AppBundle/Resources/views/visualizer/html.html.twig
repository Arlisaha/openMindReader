{% extends 'AppBundle::layout.html.twig' %}

{% block javascripts %}
    {% javascripts 
    	'@AppBundle/Resources/public/js/*'
    	'@jquery' 
	%}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>
    	$(function() {
		    CollapsibleLists.apply(true)
		});
    </script>
{% endblock %}

{% block stylesheets %}
	{% stylesheets 
		'@AppBundle/Resources/public/less/*'
		filter='lessphp'
    	output='css/all.css'
	%}
		<link href="{{ asset_url }}" rel="stylesheet" media="screen" type="text/css" />
	{% endstylesheets %}
{% endblock %}

{% block title %}
	{{ file_name }}
{% endblock %}

{% block body %}
	{{ html_string|raw }}
{% endblock %}